{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps/main",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "base": {
      "type": "library",
      "root": "libs/base",
      "entryFile": "index",
      "sourceRoot": "libs/base/src",
      "compilerOptions": {
        "tsConfigPath": "libs/base/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "mongoose": {
      "type": "library",
      "root": "libs/mongoose",
      "entryFile": "index",
      "sourceRoot": "libs/mongoose/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongoose/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "post-tags": {
      "type": "library",
      "root": "libs/post-tags",
      "entryFile": "index",
      "sourceRoot": "libs/post-tags/src",
      "compilerOptions": {
        "tsConfigPath": "libs/post-tags/tsconfig.lib.json"
      }
    },
    "posts": {
      "type": "library",
      "root": "libs/posts",
      "entryFile": "index",
      "sourceRoot": "libs/posts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/posts/tsconfig.lib.json"
      }
    },
    "posts-service": {
      "type": "application",
      "root": "apps/posts-service",
      "entryFile": "main",
      "sourceRoot": "apps/posts-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts-service/tsconfig.app.json"
      }
    },
    "authorization-service": {
      "type": "application",
      "root": "apps/authorization-service",
      "entryFile": "main",
      "sourceRoot": "apps/authorization-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authorization-service/tsconfig.app.json"
      }
    },
    "resource-policy": {
      "type": "library",
      "root": "libs/resource-policy",
      "entryFile": "index",
      "sourceRoot": "libs/resource-policy/src",
      "compilerOptions": {
        "tsConfigPath": "libs/resource-policy/tsconfig.lib.json"
      }
    },
    "accounts": {
      "type": "library",
      "root": "libs/accounts",
      "entryFile": "index",
      "sourceRoot": "libs/accounts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/accounts/tsconfig.lib.json"
      }
    }
  }
}
