        "tsConfigPath": "libs/mongoose/tsconfig.lib.json"
      }
    },
    "posts": {
      "type": "application",
      "root": "apps/posts",
      "entryFile": "main",
      "sourceRoot": "apps/posts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts/tsconfig.app.json"
      }
    },
    "post-tags": {
      "type": "application",
      "root": "apps/post-tags",
      "entryFile": "main",
      "sourceRoot": "apps/post-tags/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post-tags/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
