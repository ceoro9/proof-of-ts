{
  "monorepo": true,
  "root": "apps/main",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "post-service": {
      "type": "application",
      "root": "apps/post-service",
      "entryFile": "main",
      "sourceRoot": "apps/post-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post-service/tsconfig.app.json"
      }
    },
    "base": {
      "type": "library",
      "root": "libs/base",
      "entryFile": "index",
      "sourceRoot": "libs/base/src",
      "compilerOptions": {
        "tsConfigPath": "libs/base/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
